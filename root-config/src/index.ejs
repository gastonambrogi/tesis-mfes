<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Tesis</title>
    <meta
      http-equiv="Content-Security-Policy"
      content="default-src 'self' https: localhost:*; script-src 'unsafe-inline' 'unsafe-eval' https: localhost:*; connect-src https: localhost:* ws://localhost:*; style-src 'unsafe-inline' https:; object-src 'none';"
    />
    <meta name="importmap-type" use-injector />
    <!-- If you wish to turn off import-map-overrides for specific environments (prod), uncomment the line below -->
    <!-- More info at https://github.com/single-spa/import-map-overrides/blob/main/docs/configuration.md#domain-list -->
    <!-- <meta name="import-map-overrides-domains" content="denylist:prod.example.com" /> -->

    <!-- Shared dependencies go into this import map -->
    <script type="injector-importmap">
      {
        "imports": {
          "single-spa": "https://cdn.jsdelivr.net/npm/single-spa@6.0.3/lib/es2015/esm/single-spa.min.js",
          "@single-spa/welcome": "https://cdn.jsdelivr.net/npm/single-spa-welcome/dist/single-spa-welcome.min.js",
          "@gatesis/root-config": "https://tesis-mfes.vercel.app/gatesis-root-config.js",
          "@gatesis/header-mfe": "https://tesis-header-mfe.vercel.app/gatesis-header-mfe.js",
          "@gatesis/header-mfe-carbon": "https://tesis-header-mfe-carbon.vercel.app/gatesis-header-mfe-carbon.js",
          "@gatesis/products-mfe": "https://tesis-products-mfe.vercel.app/gatesis-products-mfe.js",
          "@gatesis/services-mfe": "https://tesis-services-mfe.vercel.app/gatesis-services-mfe.js",
          "@gatesis/about-mfe": "https://tesis-about-mfe.vercel.app/gatesis-about-mfe.js",
          "@gatesis/contact-mfe": "https://tesis-contact-mfe.vercel.app/gatesis-contact-mfe.js"
        }
      }
    </script>
    <link
      rel="preload"
      href="https://cdn.jsdelivr.net/npm/single-spa@6.0.3/lib/es2015/esm/single-spa.min.js"
      as="module"
    />
    <link rel="preload" href="https://esm.sh/react-dom" as="module" />
    <link rel="preload" href="https://esm.sh/react" as="module" />

    <!-- Add your organization's prod import map URL to this script's src  -->
    <!-- <script type="injector-importmap" src="/importmap.json"></script> -->

    <% if (isLocal) { %>
    <script type="injector-importmap">
      {
        "imports": {
          "@single-spa/welcome": "https://cdn.jsdelivr.net/npm/single-spa-welcome/dist/single-spa-welcome.min.js",
          "@gatesis/root-config": "//localhost:9000/gatesis-root-config.js",
          "@gatesis/header-mfe": "//localhost:8080/gatesis-header-mfe.js",
          "@gatesis/header-mfe-carbon": "//localhost:8501/gatesis-header-mfe-carbon.js",
          "@gatesis/products-mfe": "//localhost:8502/gatesis-products-mfe.js",
          "@gatesis/services-mfe": "//localhost:8503/gatesis-services-mfe.js",
          "@gatesis/about-mfe": "//localhost:8504/gatesis-about-mfe.js",
          "@gatesis/contact-mfe": "//localhost:8505/gatesis-contact-mfe.js"
        }
      }
    </script>
    <% } %>

    <script src="https://cdn.jsdelivr.net/npm/import-map-overrides@5.1.1/dist/import-map-overrides.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@single-spa/import-map-injector@2.0.1/lib/import-map-injector.js"></script>
    <style>
      *,
      *::before,
      *::after {
        box-sizing: border-box;
      }
      * {
        margin: 0;
      }
      body {
        line-height: 1.5;
        -webkit-font-smoothing: antialiased;
      }
      img,
      picture,
      video,
      canvas,
      svg {
        display: block;
        max-width: 100%;
      }
      input,
      button,
      textarea,
      select {
        font: inherit;
      }
      p,
      h1,
      h2,
      h3,
      h4,
      h5,
      h6 {
        overflow-wrap: break-word;
      }
      p {
        text-wrap: pretty;
      }
      h1,
      h2,
      h3,
      h4,
      h5,
      h6 {
        text-wrap: balance;
      }
    </style>
  </head>
  <body>
    <noscript> You need to enable JavaScript to run this app. </noscript>
    <main></main>
    <script>
      window.importMapInjector.initPromise.then(() => {
        import("@gatesis/root-config");
      });
    </script>
    <import-map-overrides-full
      show-when-local-storage="devtools"
      dev-libs
    ></import-map-overrides-full>
  </body>
</html>
